{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "alwaysStrict": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es5",
    "types": ["node", "jest", "k6"],
    "typeRoots": ["./node_modules/@types", "./src/@types"],
    "baseUrl": "src",
        "paths": {
          "@src/*": ["CORE_GETLOAD_PERFORMANCE/Core_getLoad_Performance/src/*"]
        }
  },
  "exclude": ["node_modules", "dist"],
  "include": ["jest.config.ts", "src/**/*", "k6_scripts/**/*"]
}
